server:
  port: 8000
    
spring:
  data:
    mongodb:
      uri: mongodb://localhost:27017/blog
      auto-index-creation: true
      repositories:
        type: auto

  application:
    name: BLOG
    
      
eureka:
  client:
    serviceUrl: 
      defaultZone: http://localhost:8761/eureka
  instance:
    hostname: localhost
    prefer-ip-address: true
    
      
